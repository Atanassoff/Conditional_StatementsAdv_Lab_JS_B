function demo(input){
    let age = Number(input[0]);
    let mf = input[1];
    switch(mf){
        case"m":
        if(age >= 16){console.log("Mr.");}
        else{console.log("Master");}
        break;
        case"f":
        if(age >= 16){console.log("Ms.");}
        else{console.log("Miss");}
        break;
        default:
        break;
    }
}
