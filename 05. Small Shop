function demo(input){
    let product = input[0];
    let city = input[1];
    let num = Number(input[2]);
    let price = 0;
    switch(product){
        case"coffee":
        if(city == "Sofia"){price = num * 0.50;}
        else if(city == "Plovdiv"){price = num * 0.40;}
        else if(city == "Varna"){price = num * 0.45;}
        break;
        case"water":
        if(city == "Sofia"){price = num * 0.80;}
        else if(city == "Plovdiv"){price = num * 0.70;}
        else if(city == "Varna"){price = num * 0.70;}
        break;
        case"beer":
        if(city == "Sofia"){price = num * 1.20;}
        else if(city == "Plovdiv"){price = num * 1.15;}
        else if(city == "Varna"){price = num * 1.10;}
        break;
        case"sweets":
        if(city == "Sofia"){price = num * 1.45;}
        else if(city == "Plovdiv"){price = num * 1.30;}
        else if(city == "Varna"){price = num * 1.35;}
        break;
        case"peanuts":
        if(city == "Sofia"){price = num * 1.60;}
        else if(city == "Plovdiv"){price = num * 1.50;}
        else if(city == "Varna"){price = num * 1.55;}
        break;
        default:
        break;
    }
    console.log(price);
}
